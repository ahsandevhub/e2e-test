{
  "name": "wemastertrade-e2e-tests",
  "version": "1.0.0",
  "type": "module",
  "description": "E2E tests for WeMasterTrade Back Office using Selenium WebDriver + Vitest + POM",
  "main": "index.js",
  "scripts": {
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:headless": "set HEADLESS=true && vitest run",
    "test:ui": "vitest --ui",
    "test:auth": "vitest run tests/auth.spec.js --reporter=verbose",
    "test:discount": "vitest run tests/create-discount.spec.js --reporter=verbose",
    "test:auth:headless": "set HEADLESS=true && vitest run tests/auth.spec.js --reporter=verbose",
    "test:discount:headless": "set HEADLESS=true && vitest run tests/create-discount.spec.js --reporter=verbose"
  },
  "keywords": [
    "e2e",
    "selenium",
    "vitest",
    "pom",
    "wemastertrade"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "selenium-webdriver": "^4.15.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "dotenv": "^16.3.1"
  }
}
